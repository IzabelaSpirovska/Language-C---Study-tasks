Task 2

#include<stdio.h>

int main()
{
	printf("\tIzabela Spirovska\n");
	printf("\tmobile: ...\n");
	printf("\tfax: ...\n");
	printf("\te-mail: ...\n");
  
	return 0;
}
-------------------------------------------------------------------------------------------------------
Task 3

#include<stdio.h>

int main()
{
	int a, b;
	a = 3;
	b = 5;
	printf("a = %d\n b = %d\n", a, b);
	
	float c, d;
	c = 7.123556789;
	d = 4.123456789;
	printf("c = %f\n d = %f\n", c, d);
	
	char e, f;
	e = 'e';
	f = 'f';
	printf("e = %c\n f = %c\n ", e, f);
	
	double g, h;
	g = 3.123456789;
	h = 2.123456789;
	printf("g = %e\n h = %e\n", g, h);
	
	
	return 0;
}
